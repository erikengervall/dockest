{
  "name": "dockest",
  "version": "0.0.3",
  "description": "Docker + Jest",
  "main": "dist/index.js",
  "author": "Erik Engervall",
  "email": "erik.engervall@gmail.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "yarn clean && tsc",
    "build:watch": "yarn clean && tsc --watch",
    "install-all": "yarn && cd example && yarn",
    "test": "cd example && yarn test",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down --volumes --rmi local",
    "seq": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:undo:all && sequelize db:seed:all",
    "jest": "jest ./dist/example",
    "jest:watch": "jest ./dist/example --watch --detectOpenHandles",
    "buildw": "webpack --config webpack.config.js",
    "buildww": "webpack --config webpack.config.js --watch"
  },
  "dependencies": {
    "deepmerge": "^3.0.0",
    "execa": "^1.0.0"
  },
  "devDependencies": {
    "@types/deepmerge": "^2.2.0",
    "@types/execa": "^0.9.0",
    "@types/jest": "^23.3.9",
    "@types/sequelize": "^4.27.32",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.3.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.4",
    "typescript-estree": "^18.1.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  }
}
