{
  "name": "dockest",
  "version": "0.1.0",
  "description": "Docker + Jest",
  "main": "dist/dockest.js",
  "author": "Erik Engervall",
  "email": "erik.engervall@gmail.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "yarn clean && tsc",
    "build:watch": "yarn clean && tsc --watch",
    "buildw": "yarn clean && webpack --config webpack.config.js",
    "buildw:watch": "yarn clean && webpack --config webpack.config.js --watch",
    "test:example": "cd example && yarn test",
    "test": "jest ./src",
    "test:unit": "yarn test",
    "test:unit:watch": "jest ./src --watch",
    "travis:install:all": "yarn && cd example && yarn",
    "travis:test:all": "yarn test:unit && yarn test:example"
  },
  "dependencies": {
    "execa": "^1.0.0"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/jest": "^24.0.0",
    "@types/sequelize": "^4.27.32",
    "jest": "^23.6.0",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.3.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.4",
    "typescript-estree": "^18.1.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  }
}
