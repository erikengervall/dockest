{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier", "tslint-eslint-rules"],
  "jsRules": {},
  "rules": {
    "quotemark": false,
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "semicolon": false,
    "member-access": false,
    "no-shadowed-variable": false,
    "arrow-parens": false,
    "no-unused-variable": true,
    "trailing-comma": [
      true,
      {
        "multiline": { "arguments": "never" }
      }
    ],
    "no-duplicate-variable": true,
    "max-line-length": false,
    "interface-over-type-literal": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["**/*.js"]
  }
}
